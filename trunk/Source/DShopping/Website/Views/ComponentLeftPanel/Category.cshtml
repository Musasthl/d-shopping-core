@model IList<Service.DTO.CategoryDto>



<div class="d-left-container">
    <div class="d-left-heading">
        Danh Mục
    </div>
    <div class="d-left-body">
        <ul>
            @{
                foreach (var cat in Model)
                {
                <li class="d-left-li ">
                    <a class="d-left-a" href="@Url.Action("ProductByCategory","Home")?categoryId=@cat.id&categoryName=@cat.name">@cat.name</a>
                </li>

                }
    
            }
        </ul>
    </div>

</div>
