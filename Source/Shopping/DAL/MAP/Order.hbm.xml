<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Order" table="[dbo].[Orders]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one insert="false" update="false" lazy="false" name="Product">
      <column name="ProductId" sql-type="int" not-null="true" />
    </many-to-one>
    <!--<property name="ProductId">
      <column name="ProductId" sql-type="int" not-null="true" />
    </property> -->
    <many-to-one insert="false" update="false" lazy="false" name="Invoice">
      <column name="InvoiceId" sql-type="int" not-null="true" />
    </many-to-one>
    <!--<property name="InvoiceId">
      <column name="InvoiceId" sql-type="int" not-null="true" />
    </property>-->
    <property name="Quantity">
      <column name="Quantity" sql-type="float" not-null="false" />
    </property>
    <property name="Price">
      <column name="Price" sql-type="float" not-null="true" />
    </property>
  </class>
</hibernate-mapping>