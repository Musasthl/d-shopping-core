<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Customer" table="[dbo].[Customers]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Email">
      <column name="Email" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Address">
      <column name="Address" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Phone">
      <column name="Phone" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Usename">
      <column name="Usename" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Password">
      <column name="Password" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one insert="false" update="false" lazy="false" name="Status">
      <column name="StatusId" sql-type="int" not-null="false" />
    </many-to-one>
    <!--<property name="StatusId">
      <column name="StatusId" sql-type="int" not-null="false" />
    </property> -->
    <many-to-one insert="false" update="false" lazy="false" name="Parent">
      <column name="ParentId" sql-type="int" not-null="false" />
    </many-to-one>
    <!-- <property name="ParentId">
      <column name="ParentId" sql-type="int" not-null="false" />
    </property>-->
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="datetime" not-null="false" />
    </property>
    <bag name="Customers" inverse="true" cascade="none">
      <key column="ParentId" />
      <one-to-many class="Customer" />
    </bag>
    <bag name="Invoices" inverse="true" cascade="none">
      <key column="CustomerId" />
      <one-to-many class="Invoice" />
    </bag>
  </class>
</hibernate-mapping>