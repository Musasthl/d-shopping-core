<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Order" table="Orders" lazy="true" >
    <id name="OrderID" column="OrderID">
      <generator class="identity" />
    </id>
    <many-to-one name="ProductID">
      <column name="ProductID" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="InvoiceID">
      <column name="InvoiceID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="Quantity">
      <column name="Quantity" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>