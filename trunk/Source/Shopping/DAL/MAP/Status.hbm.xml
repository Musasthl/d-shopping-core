<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Status" table="[dbo].[Status]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Descriptions">
      <column name="Descriptions" sql-type="nvarchar" not-null="false" />
    </property>
    <bag name="Categories" inverse="true" cascade="none">
      <key column="StatusId" />
      <one-to-many class="Category" />
    </bag>
    <bag name="Customers" inverse="true" cascade="none">
      <key column="StatusId" />
      <one-to-many class="Customer" />
    </bag>
    <bag name="Invoices" inverse="true" cascade="none">
      <key column="StatusId" />
      <one-to-many class="Invoice" />
    </bag>
    <bag name="Products" inverse="true" cascade="none">
      <key column="StatusId" />
      <one-to-many class="Product" />
    </bag>
  </class>
</hibernate-mapping>