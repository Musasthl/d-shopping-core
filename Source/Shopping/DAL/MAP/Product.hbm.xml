<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Product" table="[dbo].[Products]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one insert="false" update="false" lazy="false" name="Category">
      <column name="CategoryId" sql-type="int" not-null="false" />
    </many-to-one>
    <!--<property name="CategoryId">
      <column name="CategoryId" sql-type="int" not-null="false" />
    </property>-->
    <property name="Code">
      <column name="Code" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Description">
      <column name="Description" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Price">
      <column name="Price" sql-type="int" not-null="true" />
    </property>
    <property name="Quantity">
      <column name="Quantity" sql-type="float" not-null="true" />
    </property>
    <property name="AvailableFrom">
      <column name="AvailableFrom" sql-type="date" not-null="false" />
    </property>
    <property name="Discount">
      <column name="Discount" sql-type="int" not-null="false" />
    </property>
    <many-to-one insert="false" update="false" lazy="false" name="Status">
      <column name="StatusId" sql-type="int" not-null="true" />
    </many-to-one>
    <!--<property name="StatusId">
      <column name="StatusId" sql-type="int" not-null="true" />
    </property> -->
    <property name="Unit">
      <column name="Unit" sql-type="nvarchar" not-null="false" />
    </property>
    <bag name="Orders" inverse="true" cascade="none">
      <key column="ProductId" />
      <one-to-many class="Order" />
    </bag>
    <bag name="ProductDetails" inverse="true" cascade="none">
      <key column="ProductId" />
      <one-to-many class="ProductDetail" />
    </bag>
  </class>
</hibernate-mapping>