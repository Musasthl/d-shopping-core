<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="COM" namespace="COM.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Invoice" table="[dbo].[Invoices]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one insert="false" update="false" lazy="false" name="Customer">
      <column name="CustomerId" sql-type="int" not-null="true" />
    </many-to-one>
    <!--<property name="CustomerId">
      <column name="CustomerId" sql-type="int" not-null="true" />
    </property> -->
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="datetime" not-null="false" />
    </property>
    <many-to-one insert="false" update="false" lazy="false" name="Status">
      <column name="StatusId" sql-type="int" not-null="true" />
    </many-to-one>
    <!--<property name="StatusId">
      <column name="StatusId" sql-type="int" not-null="true" />
    </property> -->
    <bag name="Orders" inverse="true" cascade="none">
      <key column="InvoiceId" />
      <one-to-many class="Order" />
    </bag>
  </class>
</hibernate-mapping>